{
	// Place your snippets for clojure here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"user/r" : {
		"prefix": "ur",
		"body": [
			"(user/r)"
		],
		"description": "user/r"
	},
	"def" : {
		"prefix": "d",
		"body": [
			"(def ${1:name} ${2:value})"
		],
		"description": "def"
	},
	"defn" : {
		"prefix": "n",
		"body": [
			"(defn ${1:name} [${2:param}] ${3:value})"
		],
		"description": "def"
	},
	"fn" : {
		"prefix": "f",
		"body": [
			"(fn [${2:param}] ${3:value})"
		],
		"description": "def"
	},
	"keyword" : {
		"prefix": "k",
		"body": [
			":$1"
		],
		"description": "colon"
	},
	"vector" : {
		"prefix": "v",
		"body": [
			"[${1:value}]"
		],
		"description": "vector"
	},	
	"map" :{
		"prefix": "m",
		"body": [
			"{:${1:key} ${2:value}}"
		],
		"description": "map"
	},
	"set" :{
		"prefix": "s",
		"body": [
			"#{${1:value}}"
		],
		"description": "set"
	},
	"keys":{
		"prefix": "ks",
		"body": [
			"{:keys [${1:value}]}"
		],
		"description": "keys"
	},	
	"if" : {
		"prefix": "i",
		"body": [
			"(if ${1:condition} ${2:true} ${3:false})"
		],
		"description": "if"
	},
	"if-let" : {
		"prefix": "il",
		"body": [
			"(if-let [${1:key} ${2:value}] ${3:true} ${4:false})"
		],
		"description": "if-let"
	},
	"when": {
		"prefix": "w",
		"body": [
			"(when ${1:condition} ${2:value})"
		],
		"description": "when"
	},
	"when-let" :{
		"prefix": "wl",
		"body": [
			"(when-let [${1:var} ${2:value}] ${3:value})"
		],
		"description": "when-let"
	},
	"let":{
		"prefix": "l",
		"body": [
			"(let [${1:name} ${2:value}] ${3:value})"
		],
		"description": "let"
	},
}